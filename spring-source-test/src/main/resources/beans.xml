<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean class="com.itnxd.spring.bean.Person" id="person" autowire="byType">
		<property name="name" value="小人"/>
	</bean>

	<bean class="com.itnxd.spring.bean.Cat" id="cat">
		<property name="name" value="小猫"/>
	</bean>

	<bean class="com.itnxd.spring.bean.HelloFactory" id="hello"/>
</beans>